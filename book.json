{
	"root": "/",
	"gitbook" : "3.2.3",
	"structure": {
	},
	"variables": {
		"authorName": "lizhao"
	},
	"title": "LIZHAO",
	"page": {
		"title": "前端知识库(lizh)"
	},
	"plugins": [
		"-lunr",
		"-search",

		"search-plus",
		"summary",
		"theme-comscore",
		"splitter",
		"fontsettings",

		"tbfed-pagefooter",
		"theme-default",
		"pageview-count",
		"accordion",
		"sharing-plus",
		"klipse",
		"back-to-top-button",
		"chapter-fold"
	],
	"pluginsConfig": {
		"fontsettings": {
			"theme": "white",
			"family": "sans",
			"size": 2
		},
		"tbfed-pagefooter": {
			"copyright":"&copy lizhao",
			"modify_label": "文件修订时间：",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"theme-default": {
			"showLevel": true
		},
		"sharing": {
			"douban": false,
			"facebook": false,
			"google": true,
			"pocket": false,
			"qq": false,
			"qzone": true,
			"twitter": false,
			"weibo": true,
			"all": ["douban", "facebook", "google", "instapaper", "linkedin","twitter", "weibo", "messenger","qq", "qzone","viber","whatsapp"]
		}
	}
}